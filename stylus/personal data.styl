@import url(../header.css)
body
	margin 0
	padding 0
	background-color #efefef
	font-family Roboto
.header
	display -webkit-flex
	display flex
	height 35px
	line-height 35px
	background-color #fff
	margin-bottom 20px
.icon-back
	position absolute
	top 0
	left 0
	display inline-block
	width 35px
	height 35px
	background url(../../public/img/nav_back_icon@2x.png) no-repeat center
	background-size 12px 18px
.header_title
	width 100%
	display  inline-block
	font-weight bold
	letter-spacing 2px //字间距
	color #ed692b
	text-align center
.header_centent
	position absolute
	right 10px
	font-size 12px


.agent_info_bottom
	padding 0 10px
	margin-top 10px
	background #fff
	font-size 12px
	.company
		display flex
		height 33px
		line-height 33px
		font-size 14px
		border-bottom 1px solid #e1e1e1
		text-align left
		.mycompany
			flex 2
		.details
			flex 7
			font-size 12px

		.details1
			//background-size 30px
	.company1
		position relative
		&:after
			position absolute 
			content ''
			background-size contain
			right 0
			top 10px
			width 15px
			height 15px
.agent_info_card
	position absolute
	left 50%
	margin-left -100px
	display flex
	margin-top 25px
	.agent_info_li
		flex 4
		text-align center
		padding 0 4px
		.agent_card_bg
			width 90px
			margin auto
			height 64px
			background #878787
			border-radius 5px
		p
			margin  10px 0
			font-size 12px
			
	