@import url(../header.css)
body
	margin 0
	padding 0
	background-color #efefef
.header
	display -webkit-flex
	display flex
	height 35px
	line-height 35px
	background-color #fff
	margin-bottom 20px
.icon-back
	position absolute
	top 0
	left 0
	display  inline-block
	width 20px
	height 24px
	padding-top 6px
	padding-left  6px
.header_title
	width 100%
	display  inline-block
	font-weight bold
	letter-spacing 2px //字间距
	color #ed692b
	text-align center
.header_centent
	position absolute
	right 10px
	font-size 12px


.set_demand
	margin-top 10px
	height 50px
	line-height 50px
	background #fff
	padding-left 10px
	.settint_anniu
		width 50px
		height 33px
		background #ea5404
		border-radius 20px
		display inline-block
		margin-right 10px
		margin-top 8px
		float right
		.setting_smallcircle
			width 33px
			height 33px
			border-radius 20px
			background #fff
	.set_jiantou
		display inline-block
		float right
		margin-top 15px
		width 20px
		height 20px
		background url(../img/information/in_right_icon@2x.png) no-repeat
		background-size contain	
	.set_jiantou1
		display inline-block
		float right
		width 70px
		height 50px
		line-height 50px
		margin 0
		background url(../img/information/in_right_icon@2x.png) no-repeat 50px 14px
		background-size 12px	
		color #b2b2b2
.login_out
	width 100%
	height 50px
	line-height 50px
	margin-top 20px
	background #fff
	text-align center
