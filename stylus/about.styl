@import url(../header.css)
body
	margin 0
	padding 0
	background-color #efefef
.header
	display -webkit-flex
	display flex
	height 35px
	line-height 35px
	background-color #fff
	margin-bottom 20px
.icon-back
	position absolute
	top 0
	left 0
	display  inline-block
	width 20px
	height 24px
	padding-top 6px
	padding-left  6px
.header_title
	width 100%
	display  inline-block
	font-weight bold
	letter-spacing 2px //字间距
	color #ed692b
	text-align center
.header_centent
	position absolute
	right 10px
	font-size 12px


.about_bg
	width 100%
	height 215px
	text-align center
	.about_bg_logo
		margin 22px auto 0
		border-radius 10px
		width 100px
		height 100px
		background url(../../public/img/content_img_logo@2x.png) no-repeat center
		background-size cover
	.about_logo
		font-size 14px
		display inline-block
		padding-left 80px
		background url(../../public/img/content_text_logo@2x.png) no-repeat
		background-size 80px 30px
		width 70px
		height 30px
		line-height 20px
		margin-top 20px
.set_demand
	margin-top 10px
	height 50px
	line-height 50px
	background #fff
	padding-left 10px
	.set_jiantou
		display inline-block
		float right
		margin-top 15px
		width 20px
		height 20px
		background url(../../public/img/in_right_icon@2x.png) no-repeat
		background-size contain	
	.set_jiantou1
		display inline-block
		float right
		width 140px
		height 50px
		line-height 50px
		margin 0
		background url(../../public/img/in_right_icon@2x.png) no-repeat 120px 14px
		background-size 12px	
		color #979797
